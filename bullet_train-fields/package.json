{
  "name": "@bullet-train/fields",
  "version": "1.3.22",
  "description": "Bullet Train Fields",
  "source": "app/javascript/index.js",
  "module": "app/assets/javascripts/fields.esm.js",
  "main": "app/assets/javascripts/fields.js",
  "files": [
    "app/assets/javascripts/*.js",
    "app/assets/javascripts/*.js.map",
    "app/javascript/**/*.js"
  ],
  "watch": {
    "update-local-installs": "app/javascript/**/*.js"
  },
  "scripts": {
    "prebuild": "$npm_execpath run clean; $npm_execpath run copy:intl-tel-input-utils",
    "build": "microbundle",
    "copy:intl-tel-input-utils": "cp-cli node_modules/intl-tel-input/build/js/utils.js app/assets/javascripts/intl-tel-input-utils.js",
    "clean": "rimraf app/assets/javascripts",
    "prewatch": "$npm_execpath run update-local-installs",
    "watch": "npm-watch",
    "update-local-installs": "$npm_execpath run build; yalc push",
    "prerelease": "$npm_execpath run build",
    "release": "np --any-branch"
  },
  "keywords": [
    "bullet-train",
    "bullet train"
  ],
  "author": "Andrew Culver <andrew.culver@gmail.com>",
  "homepage": "https://bullettrain.co/",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/runtime": "^7.13.17",
    "@hotwired/stimulus-webpack-helpers": "^1.0.1",
    "@rails/actiontext": "^7.0.0",
    "babel-loader": "^8.2.2",
    "cp-cli": "^2.0.0",
    "emoji-mart": "^5.1.0",
    "microbundle": "^0.13.0",
    "moment-timezone": "^0.5.43",
    "np": "^7.6.0",
    "npm-watch": "^0.11.0",
    "rimraf": "^3.0.2",
    "yalc": "^1.0.0-pre.53"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.0.1",
    "@simonwep/pickr": "^1.8.1",
    "daterangepicker": "^3.1.0",
    "emoji-mart": "^5.1.0",
    "i18n-js": "^3.8.0",
    "intl-tel-input": "^17.0.8",
    "select2": "^4.0.13",
    "tributejs": "^5.1.3",
    "trix": "^2.0.1",
    "zxcvbn": "^4.4.2"
  }
}
